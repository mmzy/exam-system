var timer = null;
var count = 19;

var Rule = {
    init: function() {
        timer = setInterval(function() {
        	count--;
        	$("#timerCount").html(count);
        	if(count == 0) {
        		clearInterval(timer)
        		window.location.href = "/"
        	}
        },1000);
    }
};

$(function() {
    (function(_r) {
        _r.init()
    })(Rule);
})